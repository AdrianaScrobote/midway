{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "jest"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "dot-notation": "off",
        "no-console": "off",
        "no-return-await": "off",
        "class-methods-use-this": "off",
        "no-plusplus": "off",
        "prefer-destructuring": "off",
        "no-param-reassign": "off"
    }
}
